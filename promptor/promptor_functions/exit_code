# default config
promptor_config+=(
	[exit_code.bg]=125
	[exit_code.fg]=231
)

# must be prefix by promptor_function_
promptor_function_exit_code() {
	if [ "$__promptor_last_exit_code" -ne 0 ]; then
		echo "${promptor_config[exit_code.bg]}"
		echo "${promptor_config[exit_code.fg]}"
		echo "$__promptor_last_exit_code"
	fi
}